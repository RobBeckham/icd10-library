start lib-void-test()

client is x

rc is b

lib-dx is b

rc = $loadlib(lib-dx, "lib_DX10")

'add the dx variables
clientid             is x
effdate              is d
recdate              is d
c_dx10_dt            is x
staffid              is x
rectime              is t
recentrydate         is d
recentrytime         is t
c_dx10_reason        is x
c_dx10_stat          is x
recexpdate           is d
c_dx10_primary_a     is x
c_dx10_uniq[]        is x
10dotted             is x
9dotted              is x
c_dx10_a4_supgrp     is x
c_dx10_a4_socenv     is x
c_dx10_a4_edu        is x
c_dx10_a4_occ        is x
c_dx10_a4_housng     is x
c_dx10_a4_eco        is x
c_dx10_a4_health     is x
c_dx10_a4_legal      is x
c_dx10_a4_psyenv     is x
c_dx10_a4_none       is x
c_dx10_a5_gaf        is x
c_dx10_curr_abl      is x
c_dx10_pot_abl       is x
c_dx10_iq_score      is x
c_dx10_iq_date       is x
c_dx10_iq_test       is x
c_dx10_sq_score      is x
c_dx10_sq_date       is x
c_dx10_sq_test       is x
c_dx10_comment       is x
isConverted          is x

'get dst vars

dx_rec       is uid
dx_date      is d
dx_time      is t
dx_primary   is x
dx_reason    is x
dx10_code[]  is x
dx_rank[]    is x
dx_axis[]    is x
dx9_code[]   is x
dx_batchdt   is d
dx_cat[]     is x

'add the test client
c.ln is x
c.ln = "DX"
c.fn is x
c.fn = "TEST"
c.type is x
c.type = "AC"

clientid = "DXTEST1"

'add the test client
rc = $dbaddreg(2, clientid, c.ln, c.fn, c.type)
$dblock()
rc = $dbread(2, clientid, c.ln, c.fn, c.type)
c.ln = "DX"
c.fn = "TEST"
c.type = "AC"
rc = $dbupdate(2, clientid, c.ln, c.fn, c.type)

$infomsg1 = $format(clientid, rc, "clientid: ~~~~~~  - dbaddreg: 99, ")

temp_file is x
temp_file = "/tmp/dx-test"

irms_form is x
irms_form = "GCCDIAG"

create-date is d
create-date = "10/11/2015"

create-time is t
create-time = "11:11"

doc-desc is x
doc-desc = "testing"

rc = $putfile("<center>this is a test</center>", temp_file)

doc-id is x

rc = $putirmsdoc(doc-id,temp_file,2,clientid,irms_form,"DX10","1869",
         create-date, create-time, "text/x-cmhc-snapshot", doc-desc)

$infomsg1 = $format(doc-id, "doc id: ~~~~~~~~~~~~~~~~~~~~~")

rm-file is x
rm-file = "rm " + temp_file

rc = $unix(rm-file)

'set up the dx record
effdate = "10/1/2015"
recdate = "10/2/2015"
c_dx10_dt = "10/3/2015"
staffid = "1869"
rectime = "10:01"
recentrydate = "10/4/2015"
recentrytime = "10:02"
c_dx10_reason = "2"
c_dx10_stat = "C"
recexpdate = "10/1/2016"
c_dx10_primary_a = "1"
10dotted  = "Y"
9dotted = "Y"
c_dx10_a4_supgrp = "Y"
c_dx10_a4_socenv = "Y"
c_dx10_a4_edu    = "Y"
c_dx10_a4_occ    = "Y"
c_dx10_a4_housng = "Y"
c_dx10_a4_eco    = "Y"
c_dx10_a4_health = "Y"
c_dx10_a4_legal  = "Y"
c_dx10_a4_psyenv = "Y"
c_dx10_a4_none   = ""
c_dx10_a5_gaf    = "44"
c_dx10_curr_abl  = "2"
c_dx10_pot_abl   = "2"
c_dx10_iq_score  = "22"
c_dx10_iq_date   = "9/5/2015"
c_dx10_iq_test   = "01"
c_dx10_sq_score  = "33"
c_dx10_sq_date   = "9/6/2015"
c_dx10_sq_test   = "02"
c_dx10_comment   = "Test the Comments"
isConverted      = "N"

 rc = lib-dx:putClientDx(dx_rec, clientid,
    effdate, staffid, recdate, rectime, recentrydate, recentrytime, recexpdate,
    c_dx10_reason, c_dx10_stat, c_dx10_primary_a,
    c_dx10_uniq[], c_dx10_comment,
    10dotted, 9dotted,
    c_dx10_a4_supgrp, c_dx10_a4_socenv, c_dx10_a4_edu, c_dx10_a4_occ, c_dx10_a4_housng, c_dx10_a4_eco, c_dx10_a4_health, c_dx10_a4_legal, c_dx10_a4_psyenv, c_dx10_a4_none, c_dx10_a5_gaf,
    c_dx10_curr_abl, c_dx10_pot_abl,
    c_dx10_iq_score, c_dx10_iq_date, c_dx10_iq_test, c_dx10_sq_score, c_dx10_sq_date, c_dx10_sq_test,
    doc-id,
    isConverted)

 $infomsg9 = $infomsg9 + $format(rc, "putClientDx rc: 99, ")

 rc = lib-dx:voidClientDx(clientid, dx_rec, "10/20/2015")

 $infomsg9 = $infomsg9 + $format(rc, "voidClientDx rc: 99, ")

if dx_rec !dp then
   $errmsg3 = "Expected a dx uid but got nothing"
else
   $infomsg3 = "Got a dx uid passed"
endif

'clean up the reg
'c.dx10.rh is h
'c.dx10.dt is d
'$dblock()
'rc = $dbread(2, clientid, c.dx10.rh, c.dx10.dt)
'rc = $dbremovedst(2, clientid, c.dx10.rh, c.dx10.dt)
'$infomsg9 = $infomsg9 + $format(rc, "remove record: 99, ")

'rc = $dbremovereg(2, clientid)
'$infomsg9 = $infomsg9 + $format(rc, "remove reg: 99")
end lib-void-test
